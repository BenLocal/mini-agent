{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "App-SpringBoot",
            "request": "launch",
            "mainClass": "org.mini.agent.example.springboot.App",
            "projectName": "springboot-demo"
        },
        {
            "type": "java",
            "name": "Demo",
            "request": "launch",
            "projectName": "demo",
            "mainClass": "org.mini.agent.examples.demo.Starter"
        },
        {
            "type": "java",
            "name": "Runtime",
            "request": "launch",
            "mainClass": "org.mini.agent.runtime.Launcher",
            "projectName": "runtime",
            "args": [
                "run",
                "org.mini.agent.runtime.MainVerticle",
                "-Dmini.agent.config.path=${workspaceFolder}/.mini-agent",
                "--appId",
                "mini-agent-test",
                "--namespace",
                "public",
                "--agent-http-port",
                9999,
                "--http-port",
                8675,
                "--",
                "java",
                "-jar",
                "aa.jar"
            ]
        }
    ]
}